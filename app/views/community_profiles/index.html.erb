<h1>Listing community_profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_profiles.each do |community_profile| %>
      <tr>
        <td><%= community_profile.community.name %></td>
        <td><%= link_to 'Show', community_profile, { :class => "btn btn-default" } %></td>
        <td><%= link_to 'Edit', edit_community_profile_path(community_profile), { :class => "btn btn-default" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Load Communities', community_profiles_load_communities_path %>